  <description
    value="Identifies the condition that caused death" />
  <kind
    value="resource" />
  <abstract
    value="false" />
  <contextType
    value="resource" />
  <context
    value="Condition" />
  <type
    value="Condition" />
  <baseDefinition
    value="http://hl7.org/fhir/StructureDefinition/Condition" />
  <derivation
    value="constraint" />
  <differential>
    <element
      id="Condition">
      <path
        value="Condition" />
    </element>
    <element
      id="text">
      <path
        value="Condition.text" />
      <short
        value="text" />
      <definition
        value="SHALL contain exactly one [1..1] text (CONF:3359-255)." />
      <min
        value="1" />
      <max
        value="1" />
    </element>
    <element
      id="subject">
      <path
        value="Condition.subject" />
      <short
        value="subject" />
      <definition
        value="SHALL contain exactly one [1..1] subject (CONF:3359-253)." />
      <min
        value="1" />
      <max
        value="1" />
    </element>
    <element
      id="subject.reference">
      <path
        value="Condition.subject.reference" />
      <short
        value="reference" />
      <definition
        value="This subject SHALL contain exactly one [1..1] reference (CONF:3359-254)." />
      <min
        value="1" />
      <max
        value="1" />
    </element>
    <element
      id="onset[x]">
      <path
        value="Condition.onset[x]" />
      <slicing>
        <discriminator>
          <type
            value="value" />
          <path
            value="@type" />
        </discriminator>
        <rules
          value="open" />
      </slicing>
      <short
        value="onset[x]" />
      <definition
        value="SHOULD contain exactly one [1..1] onset[x], where onset[x] is (CONF:3359-1)" />
      <min
        value="1" />
      <max
        value="1" />
    </element>
  </differential>
